<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <StartupObject>Tools.TotalCommanderT.PluginBuilder.Builder</StartupObject>
        <RootNamespace>Tools.TotalCommanderT.PluginBuilder</RootNamespace>
        <AssemblyName>TCPluginBuilder</AssemblyName>
        <MyType>Console</MyType>
        <TargetFramework>net48</TargetFramework>
        <ApplicationIcon>Resources\TotalCommander.ico</ApplicationIcon>
    </PropertyGroup>
    <ItemGroup>
        <Reference Include="CppCodeProvider">
            <HintPath>C:\Program Files\Microsoft Visual Studio 10.0\Common7\IDE\PublicAssemblies\CppCodeProvider.dll</HintPath>
        </Reference>
        <Reference Include="Microsoft.Build" />
        <Reference Include="Microsoft.Build.Framework" />
        <Reference Include="Microsoft.Build.Utilities.v4.0" />
        <Reference Include="WindowsBase" />
    </ItemGroup>
    <ItemGroup>
        <Import Include="Microsoft.VisualBasic" />
        <Import Include="System" />
        <Import Include="System.Collections" />
        <Import Include="System.Collections.Generic" />
        <Import Include="System.Diagnostics" />
        <Import Include="System.Linq" />
        <Import Include="System.Xml.Linq" />
    </ItemGroup>
    <ItemGroup>
        <None Include="Resources\TotalCommander.ico" />
        <EmbeddedResource Include="Resources\Template.pseudozip" />
        <None Include="Resources\Usage.txt" />
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="..\..\Tools\Tools.vbproj" />
        <ProjectReference Include="..\Tools.TotalCommander.vcxproj" />
    </ItemGroup>
</Project>